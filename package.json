  "bugs": {
    "url": "https://github.com/jamieowen/labs.jamieowen.com/issues"
  },
  "homepage": "https://github.com/jamieowen/labs.jamieowen.com#readme",
  "devDependencies": {
    "babel-plugin-glslify": "^2.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "budo": "^10.0.3",
    "fs-extra": "^3.0.1",
    "glslify": "^6.1.0",
    "minimist": "^1.2.0"
  },
