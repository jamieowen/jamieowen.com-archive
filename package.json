{
  "name": "labs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "input": "src-tests/replace-me.js",
    "output": "deploy/replace-me.js"
  },
  "scripts": {
    "start": "echo 'Run a project using npm run serve --labs:input=src-tests/camera-duplicate.js'",
    "deploy": "./deploy.sh",
    "build-all": "./build.sh",
    "build": "NODE_PATH=./node_modules browserify $npm_package_config_input -t [ babelify --presets [ es2015 ] ] -t glslify  | uglifyjs -cm > $npm_package_config_output",
    "serve": "NODE_PATH=./node_modules budo $npm_package_config_input --dir static --open -- -t [ babelify --presets [ es2015 ] ] -t glslify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamieowen/labs.jamieowen.com.git"
